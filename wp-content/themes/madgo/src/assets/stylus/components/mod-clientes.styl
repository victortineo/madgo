.mod-clientes
	padding 50px 10px
	background-color #35322d
	.page-header
		h1
			color $customwhite1
			font-size 36px
			font-weight 300
	.item
		text-align center
		border-bottom 1px solid $customred3
		border-top 1px solid $customred3
		margin-bottom 15px
		img
			margin 0 auto
	.carousel-indicators
		position relative !important
		bottom 0
		li
			border 0
			background-color $customblack1
			&.active
				background-color $customblack2